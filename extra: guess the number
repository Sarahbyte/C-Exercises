// Guess the number

#include <stdio.h>

int main(void)
{
    int num = 5, guess;
    printf("Guess the number between 0 and 10 that I am thinking of: ");
    scanf("%d", &guess);

    if (guess == num) 
        printf("you got it!");
    else if (guess < num)
        printf("Too low!");
    else
        printf("Too high!");
    return 0;
}
