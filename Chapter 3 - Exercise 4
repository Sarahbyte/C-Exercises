#include <stdio.h>
// Enter a phone number of the form (xxx) xxx-xxxx, 
// have it repeated back to you as xxx.xxx.xxxx

int main(void)
{
    int num1, num2, num3;
    printf("Enter a telephone number [(xxx) xxx-xxxx]: ");
    scanf("%3d%3d%4d", &num1, &num2, &num3);
    printf("You entered %d.%d.%d", num1, num2, num3);
    return 0;
}
